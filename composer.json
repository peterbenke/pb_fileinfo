{
  "name": "peterbenke/pb-fileinfo",
  "type": "typo3-cms-extension",
  "description": "Adds infos to linked files in your website (filetype and -size).",
  "keywords": [
    "TYPO3",
    "extension",
    "frontend",
    "filesize",
    "fileinfo"
  ],
  "homepage": "https://www.typomotor.de",
  "authors": [
    {
      "name": "Peter Benke",
      "role": "Developer",
      "homepage": "https://www.typomotor.de"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "php": ">=8.1.0",
    "typo3/cms-core": "^12.4"
  },
  "autoload": {
    "psr-4": {
      "PeterBenke\\PbFileinfo\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "pb_fileinfo"
    }
  }
}